CREATE TRIGGER Atualiza_Saldo_Estoque -- nome do trigger
ON vendas -- tabela que a a=C3=A7=C3=A3o ocorrer=C3=A1
FOR INSERT -- opera=C3=A7=C3=A3o que ir=C3=A1 disparar o gatilho
AS
BEGIN -- abre bloco de comando executado em conjunto
DECLARE -- declara duas vari=C3=A1veis
@cod_prod int,
@qtde float
SELECT @cod_prod =3D codigo_produto, @qtde =3D qtde_venda
FROM INSERTED -- seleciona os dados da inser=C3=A7=C3=A3o
UPDATE estoque SET saldo =3D saldo - @qtde WHERE codigo_produto =3D @cod_pr=
od
END -- final do bloco
